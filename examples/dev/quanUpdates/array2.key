


\problem {
	\<{ int[] a; }\> (
		! a = null ->
                { a.length := 10,
                  \for int i; \if (geq(i,0) & lt(i,10)) a[i] := 0 }
                \<{ for ( int i = 0; i != 10; ++i )
                      a[i] += i; }\>
                    \forall int i; (geq(i,0) & lt(i,10) -> a[i] = i)
        )
}
