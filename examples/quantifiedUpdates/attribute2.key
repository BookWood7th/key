\javaSource "attribute_java/";

\functions {
	int f(A);
}

\problem {
	\<{ A obj; }\> (
            !obj = null & \forall A o; !o.ar@(A) = null ->
            { \for A o; o.a@(A) := f(o) ||
              \for (A o;int i) o.ar@(A)[i] := i ||
              \for A o; o.ar@(A).length := 100 }
              \<{ obj.a += obj.ar[5]; }\>
              obj.a@(A) = add(f(obj), 5)
        )
}
