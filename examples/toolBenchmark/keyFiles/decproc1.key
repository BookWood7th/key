\programVariables {
  int[] a;
}

\problem {
  \forall int x; \forall int y; (0 <= x & x <= y & y < a.length -> a[x] <= a[y])
  -> 
  \forall int x; (0 < x & x < a.length -> a[x-1] <= a[x])
}
